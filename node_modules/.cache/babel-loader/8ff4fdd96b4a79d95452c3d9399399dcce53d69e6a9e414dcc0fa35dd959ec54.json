{"ast":null,"code":"import { showBills } from '../scripts/legiscanSearch.js';\nimport TrackButton from './TrackButton.vue';\nexport default {\n  data: () => ({\n    bills: [],\n    trackedButtons: {}\n  }),\n  methods: {\n    async showBills() {\n      this.bills = await showBills(this.keywords);\n      for (var bill = 0; bill < this.bills.kength; bill++) {\n        this.makeTrackButton(this.bills[bill].bill_id);\n      }\n    },\n    trackBill(id) {\n      this.trackedButtons[id]['isTracked'] = !this.trackedButtons[id]['isTracked'];\n      this.trackedBtnText[id]['text'] = this.trackedButtons[id]['isTracked'] ? 'Tracked' : 'Track';\n    },\n    makeTrackButton(id) {\n      this.trackedButtons.put(id, {\n        isTracked: false,\n        text: 'Track'\n      });\n    }\n  },\n  components: {\n    TrackButton\n  }\n};","map":{"version":3,"names":["showBills","TrackButton","data","bills","trackedButtons","methods","keywords","bill","kength","makeTrackButton","bill_id","trackBill","id","trackedBtnText","put","isTracked","text","components"],"sources":["/root/meal-prep/src/components/HelloWorld.vue"],"sourcesContent":["<template>\n    <v-text-field\n        v-model=\"keywords\"\n        :loading=\"loading\"\n        density=\"compact\"\n        clearable\n        label=\"Keyword\"\n        variant=\"outlined\"\n        hint=\"Keywords are words that might be in the bill\"\n        prepend-inner-icon=\"mdi-magnify\"\n        single-line\n        hide-details\n        @keydown.enter.prevent=\"bills = showBills(keywords)\"\n    ></v-text-field>\n    <v-table>\n        <thead>\n            <tr>\n                <th class=\"text-left\">TRACK</th>\n                <th class=\"text-left\">BILL ID</th>\n                <th class=\"text-left\">BILL NUMBER</th>\n                <th class=\"text-left\">TITLE</th>\n                <th class=\"text-left\">LAST ACTION</th>\n            </tr>\n        </thead>\n        <tbody>\n            <tr v-for=\"bill in bills\" v-bind:key=\"bill.bill_id\">\n                <td>\n                    <track-button :bill-id=\"bill.bill_id\" />\n                </td>\n                <td>{{ bill.bill_id }}</td>\n                <td>{{ bill.bill_number }}</td>\n                <td>\n                    <a :href=\"bill.text_url\">{{ bill.title }}</a>\n                </td>\n                <td>{{ bill.last_action }}</td>\n            </tr>\n        </tbody>\n    </v-table>\n</template>\n\n<script>\nimport { showBills } from '../scripts/legiscanSearch.js';\nimport TrackButton from './TrackButton.vue';\nexport default {\n    data: () => ({ bills: [], trackedButtons: {} }),\n    methods: {\n        async showBills() {\n            this.bills = await showBills(this.keywords);\n\n            for (var bill = 0; bill < this.bills.kength; bill++) {\n                this.makeTrackButton(this.bills[bill].bill_id);\n            }\n        },\n        trackBill(id) {\n            this.trackedButtons[id]['isTracked'] =\n                !this.trackedButtons[id]['isTracked'];\n            this.trackedBtnText[id]['text'] = this.trackedButtons[id][\n                'isTracked'\n            ]\n                ? 'Tracked'\n                : 'Track';\n        },\n        makeTrackButton(id) {\n            this.trackedButtons.put(id, { isTracked: false, text: 'Track' });\n        },\n    },\n    components: {\n        TrackButton,\n    },\n};\n</script>\n"],"mappings":"AAyCA,SAASA,SAAQ,QAAS,8BAA8B;AACxD,OAAOC,WAAU,MAAO,mBAAmB;AAC3C,eAAe;EACXC,IAAI,EAAEA,CAAA,MAAO;IAAEC,KAAK,EAAE,EAAE;IAAEC,cAAc,EAAE,CAAC;EAAE,CAAC,CAAC;EAC/CC,OAAO,EAAE;IACL,MAAML,SAASA,CAAA,EAAG;MACd,IAAI,CAACG,KAAI,GAAI,MAAMH,SAAS,CAAC,IAAI,CAACM,QAAQ,CAAC;MAE3C,KAAK,IAAIC,IAAG,GAAI,CAAC,EAAEA,IAAG,GAAI,IAAI,CAACJ,KAAK,CAACK,MAAM,EAAED,IAAI,EAAE,EAAE;QACjD,IAAI,CAACE,eAAe,CAAC,IAAI,CAACN,KAAK,CAACI,IAAI,CAAC,CAACG,OAAO,CAAC;MAClD;IACJ,CAAC;IACDC,SAASA,CAACC,EAAE,EAAE;MACV,IAAI,CAACR,cAAc,CAACQ,EAAE,CAAC,CAAC,WAAW,IAC/B,CAAC,IAAI,CAACR,cAAc,CAACQ,EAAE,CAAC,CAAC,WAA