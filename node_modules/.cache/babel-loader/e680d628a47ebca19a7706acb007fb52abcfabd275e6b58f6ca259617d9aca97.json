owBills } from '../scripts/legiscanSearch.js';\nexport default {\n    data: () => ({ bills: [], trackedBtnText: 'Track', tracked: false }),\n    methods: {\n        async showBills() {\n            this.bills = await showBills(this.keywords);\n        },\n        trackBill() {\n            alert(this.trackedBtnText);\n            this.tracked = !this.tracked;\n            this.trackedBtnText = this.tracked ? 'Tracked' : 'Track';\n        },\n    },\n};\n</script>\n"],"mappings":"AA0CA,SAASA,SAAQ,QAAS,8BAA8B;AACxD,eAAe;EACXC,IAAI,EAAEA,CAAA,MAAO;IAAEC,KAAK,EAAE,EAAE;IAAEC,cAAc,EAAE,OAAO;IAAEC,OAAO,EAAE;EAAM,CAAC,CAAC;EACpEC,OAAO,EAAE;IACL,MAAML,SAASA,CAAA,EAAG;MACd,IAAI,CAACE,KAAI,GAAI,MAAMF,SAAS,CAAC,IAAI,CAACM,QAAQ,CAAC;IAC/C,CAAC;IACDC,SAASA,CAAA,EAAG;MACRC,KAAK,CAAC,IAAI,CAACL,cAAc,CAAC;MAC1B,IAAI,CAACC,OAAM,GAAI,CAAC,IAAI,CAACA,OAAO;MAC5B,IAAI,CAACD,cAAa,GAAI,IAAI,CAACC,OAAM,GAAI,SAAQ,GAAI,OAAO;IAC5D;EACJ;AACJ,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}